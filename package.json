{
  "name": "@flexis/srcset-loader",
  "version": "1.0.0",
  "description": "Highly customizable loader to generating responsive images.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/flexis-srcset-loader"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-srcset-loader/issues"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "checkSize": "size-limit",
    "test": "npm run lint && npm run jest && npm run build && npm run checkSize",
    "buildTestBundle": "NODE_ENV=test babel-node test/buildTestBundle",
    "build": "rollup -c",
    "watch": "rollup -c -w",
