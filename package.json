  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@flexis/srcset": "^1.2.1",
    "loader-utils": "^1.2.1",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "^7.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-trigen": "^1.8.0",
    "browserslist-config-trigen": "^1.1.0",
    "clean-publish": "^1.1.0",
    "eslint": "^5.11.0",
    "eslint-config-trigen": "^3.2.1",
    "eslint-plugin-jest": "^22.1.2",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "memory-fs": "^0.4.1",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^5.0.0",
    "size-limit": "^0.21.0",
    "webpack": "^4.28.2"
  },
  "files": [
    "lib"
